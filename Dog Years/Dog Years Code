#include <iostream>

int main() {
  // 2 year old dog is 21 human years
  int dog_age = 8;
  int early_years, later_years, human_years;
  
  //The first two years of a dogâ€™s life count as 21 human years
  early_years = 21;
  later_years = (dog_age - 2)* 4;
  human_years = early_years + later_years;

  std::cout << "My name is Dolce! Woof woof, I am " << human_years << " years old in human years.\n";
}
